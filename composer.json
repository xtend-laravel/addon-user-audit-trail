{
    "name": "xtend-laravel/addon-user-audit-trail",
    "description": "User Audit Trail Addon for XtendLunar",
    "type": "library",
    "keywords": [
        "laravel",
        "xtendlunar"
    ],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "XtendLunar\\Addons\\UserAuditTrail\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Adam Lee",
            "email": "adam@codelabx.ltd"
        }
    ],
    "require": {
        "php": "^8.1",
        "jenssegers/agent": "^2.6"
    },
    "extra": {
        "laravel": {
            "providers": [
                "XtendLunar\\Addons\\UserAuditTrail\\UserAuditTrailProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "config": {
        "allow-plugins": {
            "php-http/discovery": true,
            "pestphp/pest-plugin": true
        }
    }
}
